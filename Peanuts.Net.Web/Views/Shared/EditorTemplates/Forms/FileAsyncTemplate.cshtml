@model Com.QueoFlow.Peanuts.Net.Core.Domain.Documents.UploadedFile



<div class="file-uploaded__image js-fileupload-uploaded-file-preview">
    <div class="file-uploaded__overlay">
        <a class="link icon-trash js-fileupload-delete"></a>
    </div>
</div>
<span class="file-uploaded__name">Die Datei wird hochgeladen ...</span>
<div class="progress">
    <div class="js-fileuploader-progress-bar progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%"></div>
</div>