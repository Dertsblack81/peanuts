@using Com.QueoFlow.Peanuts.Net.Web.Resources
@model Com.QueoFlow.Peanuts.Net.Web.Models.Peanut.PeanutIsFixedViewModel

@{
    ViewBag.Title = "Planung für Peanut bereits abgeschlossen";
    Layout = "~/Views/Shared/_LayoutSidebar.cshtml";
}

<div class="row">
    <div class="col-xs-12">
        <p>
        Die Planung für den Peanut @Model.Peanut.Name ist bereits abgeschlossen.
        </p>
        <hr/>
        <a href="@Url.Action("Index")" class="btn btn-default">
            <span>@Resources_Web.btn_Cancel</span>
        </a>
    </div>
</div>
