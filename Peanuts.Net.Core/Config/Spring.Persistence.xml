<?xml version="1.0" encoding="utf-8" ?>

<objects xmlns="http://www.springframework.net"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:db="http://www.springframework.net/database"
         xmlns:tx="http://www.springframework.net/tx"
         xsi:schemaLocation="http://www.springframework.net 
             http://www.springframework.net/xsd/spring-objects.xsd ">

  <!-- Spring-Konfiguration der Daos. Diese sind in alphabetischer Reihenfolge (Id) zu definieren.-->

  <object id="billDao"
        type="Com.QueoFlow.Peanuts.Net.Core.Persistence.BillDao, Peanuts.Net.Core"
        autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="bookingDao"
        type="Com.QueoFlow.Peanuts.Net.Core.Persistence.BookingDao, Peanuts.Net.Core"
        autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>


  <object id="documentDao" type="Com.QueoFlow.Peanuts.Net.Core.Persistence.DocumentDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory" ref="applicationSessionFactory" />
  </object>
  
  <object id="paymentDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.PaymentDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="peanutDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.PeanutDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="peanutParticipationDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.PeanutParticipationDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="peanutParticipationTypeDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.PeanutParticipationTypeDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="proposedUserDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.ProposedUserDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>
  
  <object id="userDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.UserDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="userGroupDao"
        type="Com.QueoFlow.Peanuts.Net.Core.Persistence.UserGroupDao, Peanuts.Net.Core"
        autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>

  <object id="userGroupMembershipDao"
          type="Com.QueoFlow.Peanuts.Net.Core.Persistence.UserGroupMembershipDao, Peanuts.Net.Core"
          autowire="autodetect" >
    <property name="SessionFactory"
              ref="applicationSessionFactory" />
  </object>


</objects>